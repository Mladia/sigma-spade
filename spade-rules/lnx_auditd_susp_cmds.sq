%lnx_auditd_susp_cmds_3 = %lnx_auditd_susp_cmds_3_0 or %lnx_auditd_susp_cmds_3_1
%lnx_auditd_susp_cmds = %lnx_auditd_susp_cmds_0 or %lnx_auditd_susp_cmds_1 or %lnx_auditd_susp_cmds_2 or %lnx_auditd_susp_cmds_3
%con = %lnx_auditd_susp_cmds