%lnx_auditd_susp_cmds_1_0 = "command line" like 'chmod u+s'
%lnx_auditd_susp_cmds_1_1 = "command line" like 'chmod u+s %'
%lnx_auditd_susp_cmds_1 = %lnx_auditd_susp_cmds_1_0 or %lnx_auditd_susp_cmds_1_1
%lnx_auditd_susp_cmds_2_0 = "command line" like 'cp /bin/ksh'
%lnx_auditd_susp_cmds_2_1 = "command line" like 'cp /bin/ksh %'
