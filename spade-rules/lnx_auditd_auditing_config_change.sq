%lnx_auditd_auditing_config_change_0 = "path" like '/etc/audit/%'
%lnx_auditd_auditing_config_change_1 = "path" = '/etc/libaudit.conf'
%lnx_auditd_auditing_config_change_2 = "path" like '/etc/audisp/%'
%lnx_auditd_auditing_config_change = %lnx_auditd_auditing_config_change_0 or %lnx_auditd_auditing_config_change_1 or %lnx_auditd_auditing_config_change_2
%con = %lnx_auditd_auditing_config_change